name: main_screen
type: scene
hierarchy:
    - obj:
          - guid: OBJ-1-camera
          - children:
                - OBJ-2-sprite
entities:
    - obj:
          - guid: OBJ-1-camera
          - components:
                - component:
                      - type: "camera"
                      - data:
                            - aspectPower: 7.0
                            - isPerspective: true
                - component:
                      - type: "transform"
                      - data:
                            - position:
                                  - x: 0.0
                                  - y: 0.0
                                  - z: 0.0
                            - rotation:
                                  - x: 0.0
                                  - y: 0.0
                                  - z: 0.0
                            - scale:
                                  - x: 1.0
                                  - y: 1.0
                                  - z: 1.0
    - obj:
          - guid: OBJ-2-sprite
          - components:
                - component:
                      - type: "transform"
                      - data:
                            - position:
                                  - x: 0.0
                                  - y: 0.0
                                  - z: 0.0
                            - rotation:
                                  - x: 0.0
                                  - y: 0.0
                                  - z: 0.0
                            - scale:
                                  - x: 1.0
                                  - y: 1.0
                                  - z: 1.0
